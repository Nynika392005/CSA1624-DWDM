age <- c(23,27,39,41,47,49,50,52,54,56,57,58,60,61,62,63,64,65)
fat <- c(9.5,10.2,13.5,14.1,18.2,19.1,20.5,21.3,22.1,
         23.4,24.6,25.8,26.4,27.1,28.2,29.5,30.1,31.6)

mean(age); median(age); sd(age)
mean(fat); median(fat); sd(fat)

boxplot(age, main="Age Boxplot")
boxplot(fat, main="%Fat Boxplot")

plot(age, fat, main="Scatter Plot", xlab="Age", ylab="Fat %")

qqnorm(age); qqline(age)
qqnorm(fat); qqline(fat)
